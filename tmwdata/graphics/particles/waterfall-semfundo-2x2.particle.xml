<effect>
	<particle
		position-x = "0"
		position-y = "0"
		position-z = "0"
		>
		<emitter>
			<property name="position-x" min="-24" max="30"/>
			<property name="position-y" min="-24" max="-20"/>
			<property name="position-z" min="55" max="65"/>
			<property name="image" value="graphics/particles/waterfall-a.png"/>
