<?xml version="1.0"?>
<sprite>

	<imageset name="base" src="graphics/sprites/monster-reaper.png" width="100" height="100" />

	<action name="stand" imageset="base">
		<animation>
			<frame index="45" delay="70" />
			<frame index="45" delay="90" />
			<frame index="45" delay="120" />
			<frame index="45" delay="140" />
			<frame index="45" delay="150" />
			<frame index="45" delay="140" />
			<frame index="45" delay="120" />
			<frame index="45" delay="90" />
		</animation>
	</action>

	<action name="walk" imageset="base">
		<animation>
			<frame index="45" delay="70" />
			<frame index="45" delay="90" />
			<frame index="45" delay="120" />
			<frame index="45" delay="140" />
			<frame index="45" delay="150" />
			<frame index="45" delay="140" />
			<frame index="45" delay="120" />
			<frame index="45" delay="90" />
		</animation>

	</action>

	<action name="attack" imageset="base">
		<animation>
			<sequence start="45" end="45" delay="75" />
			<frame index="45" delay="500" />
		  	<end />
		</animation>
	</action>
	
		<action name="dead" imageset="base">
		<animation>
			<frame index="46"/>
		  	<end />
		</animation>
	</action>

</sprite>
